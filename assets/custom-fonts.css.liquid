

@font-face {
    font-family: 'Arial Rounded MT';

    src: url({{'ArialRoundedMTBold.eot'| file_url }});
    src: url({{ 'ArialRoundedMTBold.eot?#iefix'| file_url }}) format('embedded-opentype'),
        url({{'ArialRoundedMTBold.woff2'| file_url }}) format('woff2'),
        url({{'arialroundedmtbold.ttf'| file_url }}) format('truetype'),
        url({{'ArialRoundedMTBold.woff' | file_url }}) format('woff');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}